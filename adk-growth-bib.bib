	file = {Sendak and McEvoy - 2013 - Vermont Stumpage Price Trends Revisited With Comp.pdf:C\:\\Users\\Neal\\Zotero\\storage\\7VG39CW2\\Sendak and McEvoy - 2013 - Vermont Stumpage Price Trends Revisited With Comp.pdf:application/pdf}
